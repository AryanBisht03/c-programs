#include<conio.h>
using namespace std;
class A
{
public:
void fun()
 {
 	cout<<"hy i am the parent function"<<endl;
	 }	
};

class B:public A
{
public:
void fun1()
 {
 	cout<<"hy i am the parent function ka pehla beta B"<<endl;
	 }	
};

class C:public A
{
public:
void fun2()
 {
 	cout<<"hy i am the parent function ka doosra beta C"<<endl;
	 }	
};

